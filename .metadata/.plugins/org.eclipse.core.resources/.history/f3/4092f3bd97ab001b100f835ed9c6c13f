package service;

import java.util.ArrayList;
import java.util.List;

public class PrintService {
	
	private List<Integer> list = new ArrayList<>();
	
	public void addValue(int value) {
		list.add(value);
	}
	
	public int first() {
		return list.get(0);
	}
	
	private void print() {
		System.out.println(list.toString());
	}
}
